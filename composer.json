{
    "name": "aaroncadrian/larahelp",
    "description": "Additional helper functions for Laravel",
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "illuminate/support": ">=5.0,<5.6",
        "illuminate/validation": ">=5.0,<5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "author": {
        "name": "Aaron Adrian",
        "email": "aaroncadrian@gmail.com"
    },
    "autoload": {
        "files": [
            "src/larahelp.php"
        ],
        "psr-4": {
            "AaronAdrian\\Larahelp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronAdrian\\Larahelp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronAdrian\\Larahelp\\LarahelpServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}